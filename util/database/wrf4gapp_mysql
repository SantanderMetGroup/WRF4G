BEGIN;
CREATE TABLE `wrf4gapp_experiment_db` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
    `name` varchar(100) NOT NULL,
    `end_date` datetime,
    `start_date` datetime,
    `status` varchar(1)
)
;
CREATE TABLE `wrf4gapp_realization_db` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
    `name` varchar(100) NOT NULL,
    `exp_id` integer,
    `end_date` datetime,
    `start_date` datetime
)
;
ALTER TABLE `wrf4gapp_realization_db` ADD CONSTRAINT `exp_id_refs_id_60fdbf2b` FOREIGN KEY (`exp_id`) REFERENCES `wrf4gapp_experiment_db` (`id`);
CREATE TABLE `wrf4gapp_chunk_db` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
    `name` varchar(100) NOT NULL,
    `rea_id` integer,
    `end_date` datetime,
    `start_date` datetime,
    `current_date` datetime,
    `wps_file` bool,
    `status` varchar(1)
)
;
ALTER TABLE `wrf4gapp_chunk_db` ADD CONSTRAINT `rea_id_refs_id_45108a64` FOREIGN KEY (`rea_id`) REFERENCES `wrf4gapp_realization_db` (`id`);
CREATE TABLE `wrf4gapp_file_type_db` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
    `type_file` varchar(3) NOT NULL,
    `freq_h` integer
)
;
CREATE TABLE `wrf4gapp_file_db` (
    `id` integer AUTO_INCREMENT NOT NULL PRIMARY KEY,
    `type_file_id` integer NOT NULL,
    `path` varchar(100),
    `rea_id` integer,
    `end_date` datetime,
    `start_date` datetime
)
;
ALTER TABLE `wrf4gapp_file_db` ADD CONSTRAINT `rea_id_refs_id_75ce8aee` FOREIGN KEY (`rea_id`) REFERENCES `wrf4gapp_realization_db` (`id`);
ALTER TABLE `wrf4gapp_file_db` ADD CONSTRAINT `type_file_id_refs_id_7633bd8b` FOREIGN KEY (`type_file_id`) REFERENCES `wrf4gapp_file_type_db` (`id`);
CREATE INDEX `wrf4gapp_realization_db_exp_id` ON `wrf4gapp_realization_db` (`exp_id`);
CREATE INDEX `wrf4gapp_chunk_db_rea_id` ON `wrf4gapp_chunk_db` (`rea_id`);
CREATE INDEX `wrf4gapp_file_db_type_file_id` ON `wrf4gapp_file_db` (`type_file_id`);
CREATE INDEX `wrf4gapp_file_db_rea_id` ON `wrf4gapp_file_db` (`rea_id`);
COMMIT;

