#
#   Experiment configuration
#
experiment_name      = "single"

#   Simulation domain
max_dom              = 1
domain_name          = "Santander_50km"

#   Input data
extdata_vtable       = "GFS"      # Vtables must exist as Vtable.[input_extdata]
extdata_path         = "${WRF4G_INPUT}/NCEP/GFS"
extdata_interval     = 21600      # Seconds between global analysis input times
extdata_preprocessor = "default"  # It has to be under $WRF4G_APPS/pre-processors directory

#   Output data
postprocessor        = "SFC"      # It has to be under $WRF4G_APPS/post-processors directory
wrfout_name_end_date = 0

#   Experiment time-specification
rerun                = 0
start_date           = "2011-08-28_12:00:00"
end_date             = "2011-08-30_00:00:00"
chunk_size_h         = 12

multiple_dates       = 0
	simulation_interval_h    = 0
	simulation_length_h      = 0

multiple_parameters  = 0
	multiparams_variables    = ""
	multiparams_nitems       = ""
	multiparams_combinations = ""
	multiparams_labels       = ""

#   Debugging
clean_after_run      = 1
save_wps             = 0

#   Parallel configuration  
real_parallel        = 0
wrf_parallel         = 1

#
#   WRF-namelist parameters. Override namelist.input variables here
#

namelist_version       = "3.3.1"

#   Single valued:
NI_restart_interval    = 720
NI_spec_bdy_width      = 10
NI_spec_zone           = 1
NI_relax_zone          = 9
NI_feedback            = 0

#   One value per domain:
NIM_history_interval   = 180,60,60,60 # minutes
NIM_frames_per_outfile = 3,12,12,12

#   One value per domain; but all equal (provide a single value here):
NIN_e_vert             = 28
NIN_mp_physics         = 4   # 1: Kessler, 2: Lin, 3: WRF S-M 3, 4: WRF S-M 5, 5: Eta, 6: WRF S-M 6, 7: Goddard, 8: Thompson, 10: Morrison
NIN_radt               = 15
NIN_ra_lw_physics      = 3
NIN_ra_sw_physics      = 3

#
#   CLWRF  
#
NI_clwrf_variables__time_control      = 1
NI_gust_wind__time_control            = 0

#   Extreme temperatures and wind
NI_auxhist3_outname__time_control     = 'wrfxtrm_d<domain>_<date>'
NI_io_form_auxhist3__time_control     = 2
NIM_auxhist3_interval__time_control   = 180,180,180,180
NIM_frames_per_auxhist3__time_control = 999,999,999,999