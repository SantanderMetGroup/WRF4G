#!/bin/bash

# wrf4g_env load WRF4G shell environment variables

if test -z $WRF4G_LOCATION; then
  WRF4G_BIN_LOCATION=`dirname $0`
  export WRF4G_LOCATION=`dirname $WRF4G_BIN_LOCATION`
fi


export GW_LOCATION=$WRF4G_LOCATION/opt/gw_drm4g
export MYSQL_LOCATION=$WRF4G_LOCATION/opt/mysql-5.5
export PATH=$GW_LOCATION/bin:$PATH
export PYTHONPATH=$WRF4G_LOCATION/lib/python:$PYTHONPATH
